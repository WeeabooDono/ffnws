{% extends 'base.html.twig' %}

{% block body %}
    <h1>UtilisateurRole list</h1>

    <table>
        <thead>
            <tr>
                <th>Idpersonne</th>
                <th>Idcompetition</th>
                <th>Idrole</th>
                <th>Rang</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for utilisateurrole in utilisateurroles %}
            <tr>
                <td><a href="{{ path('personne_show', { 'idPersonne': utilisateurrole.idPersonne.idPersonne }) }}">{{ utilisateurrole.idPersonne.nom }}</a></td>
                <td><a href="{{ path('competition_show', { 'idCompetition': utilisateurrole.idCompetition.idCompetition }) }}">{{ utilisateurrole.idCompetition.nom }}</a></td>
                <td><a href="{{ path('role_show', { 'idRole': utilisateurrole.idRole.idRole }) }}">{{ utilisateurrole.idRole.nom }}</a></td>
                <td>{{ utilisateurrole.rang }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('utilisateurrole_show', { 'idRole': utilisateurrole.idRole.idRole, 'idPersonne': utilisateurrole.idPersonne.idPersonne, 'idCompetition': utilisateurrole.idCompetition.idCompetition }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('utilisateurrole_edit', { 'idRole': utilisateurrole.idRole.idRole, 'idPersonne': utilisateurrole.idPersonne.idPersonne, 'idCompetition': utilisateurrole.idCompetition.idCompetition }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('utilisateurrole_new') }}">Create a new utilisateurrole</a>
        </li>
    </ul>
{% endblock %}
